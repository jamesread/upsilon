[Unit]
Description=upsilon
After=syslog.target network.target remote-fs.target nss-lookup.target

[Service]
User=upsilon
StandardOutput=syslog
StandardError=syslog
SyslogIdentifier=upsilon
ExecStart=/usr/bin/java -Djava.net.preferIPv4Stack=true -jar /usr/share/upsilon/upsilon.jar $OPTIONS

[Install]  
WantedBy=multi-user.target
